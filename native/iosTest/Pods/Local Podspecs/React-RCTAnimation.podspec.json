{
  "name": "React-RCTAnimation",
  "version": "0.72.15",
  "summary": "A native driver for the Animated API.",
  "homepage": "https://reactnative.dev/",
  "license": "MIT",
  "authors": "Meta Platforms, Inc. and its affiliates",
  "platforms": {
    "ios": "12.4"
  },
  "compiler_flags": "-DFOLLY_NO_CONFIG -DFOLLY_MOBILE=1 -DFOLLY_USE_LIBCPP=1 -Wno-comma -Wno-shorten-64-to-32 -Wno-nullability-completeness",
  "source": {
    "git": "https://github.com/facebook/react-native.git",
    "tag": "v0.72.15"
  },
  "source_files": "**/*.{h,m,mm}",
  "preserve_paths": [
    "package.json",
    "LICENSE",
    "LICENSE-docs"
  ],
  "header_dir": "RCTAnimation",
  "pod_target_xcconfig": {
    "USE_HEADERMAP": "YES",
    "CLANG_CXX_LANGUAGE_STANDARD": "c++17",
    "HEADER_SEARCH_PATHS": "\"$(PODS_ROOT)/RCT-Folly\" \"${PODS_ROOT}/Headers/Public/React-Codegen/react/renderer/components\" \"${PODS_CONFIGURATION_BUILD_DIR}/React-Codegen/React_Codegen.framework/Headers\""
  },
  "dependencies": {
    "RCT-Folly": [
      "2021.07.22.00"
    ],
    "React-Codegen": [
      "0.72.15"
    ],
    "RCTTypeSafety": [
      "0.72.15"
    ],
    "ReactCommon/turbomodule/core": [
      "0.72.15"
    ],
    "React-jsi": [
      "0.72.15"
    ],
    "React-Core/RCTAnimationHeaders": [
      "0.72.15"
    ]
  }
}
